<Window x:Class="demoJSON.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:demoJSON"
        mc:Ignorable="d"
        Title="JSOn Demo" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Button x:Name="button" Content="Hae Json dataa" Click="button_Click"></Button>
            <TextBlock x:Name="txtJson" Height="35" TextWrapping="Wrap" Text="textbox"></TextBlock>
            <DataGrid x:Name="dgData" Margin="10,60,0,0" ItemsSource="{Binding}"></DataGrid>
        </StackPanel>
        
    </Grid>
</Window>
